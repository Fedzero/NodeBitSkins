    <body>
        <div style='text-align: center'>
        <form class="form-horizontal">
<fieldset>

<!-- Form Name -->
<legend>Form Name</legend>

<!-- Multiple Checkboxes (inline) -->
<div class="form-group">
  <label class="col-md-4 control-label" for="checkboxes">Block</label>
  <div class="col-md-4">
    <label class="checkbox-inline" for="checkboxes-0">
      <input type="checkbox" name="checkboxes" id="checkboxes-0" value="Knives">
      Knives
    </label>
    <label class="checkbox-inline" for="checkboxes-1">
      <input type="checkbox" name="checkboxes" id="checkboxes-1" value="Souvenir">
      Souvenir
    </label>
    <label class="checkbox-inline" for="checkboxes-2">
      <input type="checkbox" name="checkboxes" id="checkboxes-2" value="Case">
      Case
    </label>
    <label class="checkbox-inline" for="checkboxes-3">
      <input type="checkbox" name="checkboxes" id="checkboxes-3" value="Key">
      Key
    </label>
    <label class="checkbox-inline" for="checkboxes-4">
      <input type="checkbox" name="checkboxes" id="checkboxes-4" value="StatTrack">
      StatTrack
    </label>
  </div>


<!-- Text input-->
<div class="form-group">
  <label class="col-md-4 control-label" for="RegExStringInput">RegExString</label>  
  <div class="col-md-5">
  <input id="RegExStringInput" name="RegExStringInput" type="text" placeholder="placeholder" class="form-control input-md" required="">
    
  </div>
</div>

<!-- Button -->
<div class="form-group">
  <label class="col-md-4 control-label" for="saveSettingsBtn">Save Settings</label>
  <div class="col-md-4">
    <button id="saveSettingsBtn" name="saveSettingsBtn" class="btn btn-success">Save</button>
  </div>
</div>
    </div>

</fieldset>
</form>

